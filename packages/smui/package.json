{
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@material/typography": "^14.0.0",
		"@mdi/js": "^7.4.47",
		"@smui/button": "^8.0.3",
		"@smui/list": "^8.0.3",
		"@smui/tab": "^8.0.3",
		"@smui/tab-bar": "^8.0.3",
		"@smui/top-app-bar": "^8.0.3",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.42.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tanstack/svelte-query": "^5.89.0",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/svelte": "^5.2.8",
		"@vitest/coverage-v8": "^3.2.4",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.3",
		"globals": "^16.4.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"smui-theme": "^8.0.3",
		"svelte": "^5.39.2",
		"svelte-check": "^4.3.1",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.0",
		"vite": "^7.1.6",
		"vitest": "^3.2.4",
		"web-vitals": "^5.1.0"
	},
	"name": "@diagnostics/smui",
	"overrides": {
		"cookie": "^1.0.2"
	},
	"private": true,
	"scripts": {
		"build": "vite build",
		"check": "npm-run-all check:*",
		"check:svelte-kit": "svelte-kit sync",
		"check:svelte": "svelte-check --tsconfig ./tsconfig.json",
		"dev": "vite dev",
		"format": "prettier --write .",
		"lint": "npm-run-all lint:*",
		"lint:prettier": "prettier --check .",
		"lint:eslint": "eslint --concurrency=auto .",
		"prepare": "svelte-kit sync || echo ''",
		"preview": "vite preview",
		"smui-theme-prepare": "npm-run-all smui-theme-prepare:*",
		"smui-theme-prepare:dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"smui-theme-prepare:light": "smui-theme compile static/smui.css -i src/theme",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:watch": "vitest",
		"watch": "npm-run-all watch:*",
		"watch:svelte-kit": "svelte-kit sync",
		"watch:svelte": "svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"type": "module",
	"version": "0.0.1"
}
