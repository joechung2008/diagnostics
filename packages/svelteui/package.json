{
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@svelteuidev/composables": "^0.15.7",
    "@svelteuidev/core": "^0.15.7",
    "@tanstack/svelte-query": "^5.90.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/svelte": "^5.2.8",
    "@tsconfig/svelte": "^5.0.5",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.4",
    "lucide-svelte": "^0.544.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^4.2.20",
    "svelte-check": "^4.3.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^5.4.20",
    "vitest": "^3.2.4",
    "web-vitals": "^5.1.0"
  },
  "name": "@diagnostics/svelteui",
  "overrides": {
    "esbuild": "^0.25.10"
  },
  "private": true,
  "scripts": {
    "build": "vite build",
    "check": "run-s check:*",
    "check:svelte": "svelte-check --tsconfig ./tsconfig.app.json",
    "check:tsc": "tsc -p tsconfig.node.json",
    "dev": "vite",
    "format": "prettier --write .",
    "lint": "run-s lint:*",
    "lint:check": "npm run check",
    "lint:eslint": "eslint --concurrency=auto .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "type": "module",
  "version": "0.0.0"
