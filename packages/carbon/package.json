{
	"author": "Joe Chung",
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.42.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tanstack/svelte-query": "^5.89.0",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/svelte": "^5.2.8",
		"@types/node": "^22.18.6",
		"@vitest/coverage-v8": "^3.2.4",
		"carbon-components-svelte": "^0.89.7",
		"carbon-icons-svelte": "^13.5.0",
		"carbon-preprocess-svelte": "^0.11.11",
		"debug": "^4.4.3",
		"eslint": "^9.35.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.3",
		"globals": "^16.4.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.39.2",
		"svelte-check": "^4.3.1",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.0",
		"vite": "^7.1.6",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^3.2.4",
		"web-vitals": "^5.1.0"
	},
	"name": "@diagnostics/carbon",
	"overrides": {
		"cookie": "^0.7.0"
	},
	"private": true,
	"scripts": {
		"build": "vite build",
		"check": "run-s check:*",
		"check:svelte-check": "svelte-check --tsconfig ./tsconfig.json",
		"check:svelte-kit": "svelte-kit sync",
		"dev": "vite dev",
		"format": "prettier --write .",
		"lint": "run-s lint:*",
		"lint:eslint": "eslint --concurrency=auto .",
		"lint:prettier": "prettier --check .",
		"prepare": "svelte-kit sync || echo ''",
		"preview": "vite preview",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:watch": "vitest",
		"watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"watch:svelte-check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"watch:svelte-kit": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"type": "module",
	"version": "0.0.1"
}
