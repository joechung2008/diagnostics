{
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@skeletonlabs/skeleton": "^3.2.2",
		"@skeletonlabs/skeleton-svelte": "^1.5.2",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.43.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.13",
		"@tanstack/svelte-query": "^5.90.2",
		"@testing-library/jest-dom": "^6.8.0",
		"@testing-library/svelte": "^5.2.8",
		"@vitest/coverage-v8": "^3.2.4",
		"eslint": "^9.36.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"svelte": "^5.39.6",
		"svelte-check": "^4.3.2",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.44.1",
		"vite": "^7.1.7",
		"vite-plugin-devtools-json": "^1.0.0",
		"vitest": "^3.2.4",
		"web-vitals": "^5.1.0"
	},
	"name": "@diagnostics/skeleton",
	"overrides": {
		"cookie": "^1.0.2"
	},
	"private": true,
	"scripts": {
		"build": "vite build",
		"check": "run-s check:*",
		"check:sync": "svelte-kit sync",
		"check:ts": "svelte-check --tsconfig ./tsconfig.json",
		"dev": "vite dev",
		"format": "prettier --write .",
		"lint": "run-s lint:*",
		"lint:eslint": "eslint --concurrency=auto .",
		"lint:prettier": "prettier --check .",
		"prepare": "svelte-kit sync || echo ''",
		"preview": "vite preview",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:watch": "vitest",
		"watch": "run-s check:sync watch:ts",
		"watch:ts": "svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"type": "module",
	"version": "0.0.1"
}
