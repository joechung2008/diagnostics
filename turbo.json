{
  "$schema": "https://turborepo.com/schema.v2.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        ".output/**",
        ".svelte-kit/**",
        "build/**",
        "dist/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "lint": {},
    "test": {
      "dependsOn": ["^build"]
    },
    "start": {
      "cache": false
    }
  }
}
